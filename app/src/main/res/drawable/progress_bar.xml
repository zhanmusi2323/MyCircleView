<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dip" />
            <gradient
                android:centerColor="#3f3b56"
                android:centerY="0.5"
                android:endColor="#3f3b56"
                android:startColor="#3f3b56"
                android:type="linear" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient
                    android:centerColor="#10c995"
                    android:centerX="1.0"
                    android:endColor="#10c995"
                    android:startColor="#3f3b56"
                    />
            </shape>
        </clip>
    </item>
</layer-list>
